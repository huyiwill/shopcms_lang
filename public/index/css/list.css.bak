/****category_more**/
.category_more{
	height:30px;
	text-align: center;
	line-height: 30px;
	position: relative;
	top:-1px;
	border-top:1px solid #DDDDDD;
}
.category_more .cm_wrap{
	cursor: pointer;
	border:1px solid #DDDDDD;
	border-top:1px solid #ffffff;
	display: inline-block;
	padding-left: 10px;
	padding-right: 30px;
	height:30px;
	line-height: 30px;
	position: relative;
	top:-1px;
}
.category_more:hover{
	border-top:1px solid #E8303F;
}
.category_more:hover .cm_wrap{
	color:#E8303F;
	border-color:#E8303F;
	border-top:1px solid #ffffff;
	position: relative;
	top:-1px;
}
.category_more .cm_wrap i{
	position: absolute;
	width:20px;
	height:20px;
	right:5px;
	top:5px;
	display: inline-block;
	transition: background-position 0.2s;
	background: url(../images/cate_pic_001.png) no-repeat 3px 8px;
}
