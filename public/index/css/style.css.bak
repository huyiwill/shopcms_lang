#arealist{position:relative;float:left;z-index:2;height:26px;margin-right:6px}
#arealist dl,#arealist dt,#arealist dd{float:none;color:#999}
#arealist dl{position:absolute;top:23px;z-index:1;width:358px;width:358px;padding:5px;border:1px solid #ddd;background:#fff;display:none;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#arealist dt{padding:6px 0 10px;color:#999}
#arealist dd{padding-bottom:5px;line-height:18px}
#arealist a:link,#arealist a:visited{color:#005aa0}
#arealist a:hover,#arealist a:active{background:#005aa0;color:#fff}
#arealist .text{float:left;+float:none;_float:left;position:relative;top:0;z-index:2;height:23px;background:#fff;border:1px solid #CECBCE;padding:0 20px 0 4px;line-height:23px;overflow:hidden}
#arealist .text b{display:block;position:absolute;top:0;right:0;overflow:hidden;width:17px;height:24px;background:url(../images/btn20121210.png) 0 0 no-repeat}
#arealist .close{display:none;position:absolute;z-index:2;top:19px;left:468px;cursor:pointer;width:17px;height:17px;background:url(../images/20120418.png) no-repeat -40px 0}
#arealist a:hover{background:#cc0000;color:#fff;cursor:pointer}

#arealist.hover .close{z-index:9999;display:block}

#store-prompt{clear:left;line-height:25px}
#store-prompt strong{font-size:14px}
#store-prompt a{color:#005EAA}
*html #arealist .i-storeinfo,*html #arealist .i-storeinfo div{float:left}
