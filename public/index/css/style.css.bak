#arealist .content{display:none;z-index:999;overflow:auto;position:absolute;top:23px;left:0px;border:1px solid #cecbce;width:449px;padding:15px;background:#fff;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#arealist .content select{float:left;width:120px;border:1px solid #cecbce;margin-right:15px}
#arealist .content .select3{margin-right:0}
#arealist .content em{font-style:normal}
#arealist.hover .content,#arealist.hover .close{display:block}
#ul-list{position:relative;margin-bottom:0}
#ul-list .tab{z-index:999;width:100%;height:25px;float:left;border-bottom:2px solid #cc0000;overflow:visible;*overflow:hidden}
#ul-list .tab li{float:left;clear:none;height:23px;padding:1px 1px 0;border:1px solid #ddd;border-bottom:0;margin-right:3px;background-color:#fff;line-height:22px;text-decoration:none}
#ul-list .tab .curr{position:relative;height:25px;padding:0;border:2px solid #cc0000;border-bottom:0}
#ul-list .tab a{position:relative;float:left;height:23px;padding:0 20px 1px 10px;line-height:23px;text-align:center;text-decoration:none;cursor:pointer;color:#005AA0;outline:0;*blr:expression(this.onFocus=this.blur())}
#ul-list .tab a:hover{background:0;color:#005AA0}
#ul-list .tab a i{position:absolute;right:5px;top:10px;*top:9px;display:block;width:7px;height:5px;overflow:hidden;background:url(../images/20130606B.png) no-repeat -76px -34px;opacity:.5;filter:alpha(opacity=50)}
#ul-list .tab .curr i,#ul-list .tab a:hover i{opacity:1;filter:alpha(opacity=100)}
