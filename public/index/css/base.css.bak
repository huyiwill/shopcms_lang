/***********************  header ***************************/
.header_in{height: 34px;line-height:34px;   border-bottom: 1px solid #e5e5e5;
    background-color: #f4f4f4; }
.header_in .header_lef p{line-height:34px;  color:#777777; }
.header_in .header_lef p span{padding:0 2px;  height:18px;  line-height:18px;  background:#ff3333;  color:#fff;  border-radius:3px; } 
.header_in .header_lef p .sc_a{color:#333333;  display:inline-block; }
.header_in .header_lef p .tc_a{color:#333333;  display:inline-block; }
.header_in .header_lef p .tc_a:hover{color:#fd6721;}
.header_in .header_rgt{font-size: 0;  text-align: right;  height: 30px;   }
.header_in .header_rgt a {color: #666666!important; }
.header_in .header_rgt dl {text-align: left; 	vertical-align: top;  letter-spacing: normal;  word-spacing: normal;  display:inline-block;  width:74px;  height: 30px;  position: relative;  z-index: 999; }
.header_in .header_rgt dl dt {font-size: 12px; 	line-height: 20px; 	height: 20px; 	padding: 5px 0; 	position: absolute; 	z-index: 2; 	top: 0; 	left: 12px; }
.header_in .header_rgt dl dt a{display:inline-block; }
.header_in .header_rgt dl dt a:hover {text-decoration: none;  color: #e8303f; }
.header_in .header_rgt dl dt i {background: url(../images/top_icon.gif) no-repeat -1px -34px;  vertical-align: middle;  display: inline-block;  width: 7px; 	height: 4px;  margin-left: 6px; 	-webkit-transition: .2s ease-in; 	-moz-transition: -webkit-transform .2s ease-in; 	-o-transition: -webkit-transform .2s ease-in; 	transition: .2s ease-in; }
.header_in .header_rgt dl:hover dt i {FILTER: progid:DXImageTransform.Microsoft.BasicImage(rotation=2); 	-moz-transform: rotate(180deg); 	-moz-transform-origin: 50% 30%; 	-webkit-transform: rotate(180deg); 	-webkit-transform-origin: 50% 30%; 	-o-transform: rotate(180deg); 	-o-transform-origin: 50% 30%; 	transform: rotate(180deg); 	transform-origin: 50% 30%; }
.header_in .header_rgt dl dd {	background-color: #FFF; 	display: none; 	width: 98px;  border: solid 1px #F0F0F0; 	position: absolute; 	z-index: 1; 	top: 0; 	left: 0; }
.header_in .header_rgt dl:hover dd {display: block; }
.header_in .header_rgt dl dd ul {width:98px;  margin: 30px 0 0 0; }
.header_in .header_rgt dl dd ul li a {font-size: 12px;  line-height: 24px; display: block; clear: both;  padding: 2px 0 2px 11px; }
.header_in .header_rgt dl dd ul li a:hover {text-decoration: none;  background-color: #F7F7F7; }
.header_in .header_rgt dl.weixin dd h4 {line-height: 18px; 	color: #fc6722; text-align: center; margin: 30px 0 5px 0;  font-size: 12px; }
.header_in .header_rgt dl.weixin dd img {max-width: 96px;  max-height: 96px;  margin:5px 0 5px 10px; }
.header_in .header_rgt dl dt em {background-image: url(../images/top_icon.gif); 	 background-repeat: no-repeat; 	vertical-align: middle; 	display: inline-block; 	width: 16px;  height: 15px; 	margin: 0 4px 0 0;  transition: all .2s;  -webkit-transition: all .2s; }
.header_in .header_rgt dl:hover dt em {	background-image: url(../images/top_icon.gif); 	background-repeat: no-repeat; }
.header_in .header_rgt dl dt em.ico_tel {background-position: -1px -1px; }
.header_in .header_rgt dl:hover dt em.ico_tel {	background-position: -1px -17px; }
.header_in .header_rgt dl dt em.ico_shop {background-position: -18px -1px; }
.header_in .header_rgt dl:hover dt em.ico_shop {background-position: -18px -17px; }
.header_in .header_rgt dl dt em.ico_order {	background-position: -35px -1px; }
.header_in .header_rgt dl:hover dt em.ico_order {background-position: -35px -17px; }
.header_in .header_rgt dl dt em.ico_store {	background-position: -52px -1px; }
.header_in .header_rgt dl:hover dt em.ico_store {background-position: -52px -17px; }
.header_in .header_rgt dl dt em.ico_service {background-position: -69px -1px; }
.header_in .header_rgt dl:hover dt em.ico_service {	background-position: -69px -17px; }
.header_in .header_rgt dl dt em.weixin {background-position: -86px -1px; }
.header_in .header_rgt dl:hover dt em.weixin  {	background-position: -86px -17px; }
.header_in .header_rgt dl.down_app {font-size: 12px; }
.header_in .header_rgt dl.down_app dd {width: 171px; height:78px; padding:6px 11px 4px 6px; left: 0; 	top: 30px; 	text-align: center; }
.header_in .header_rgt dl.down_app .qrcode {	text-align: center; 	float: left; }
.header_in .header_rgt dl.down_app .qrcode img {display: block; 	max-width: 120px; 	max-height: 120px; }
.header_in .header_rgt dl.down_app .hint {	width: 86px; 	float: right; 	text-align: center; font-size:9px; color:#333333;}
.header_in .header_rgt dl.down_app .hint h4 {font-size: 12px; color: #db2a2f; }
.header_in .header_rgt dl.down_app .addurl {width: 90px; 		clear: right; 	margin: 6px 4px 0 0; 	text-align: left; }
.header_in .header_rgt dl.down_app .addurl a {color: #666666; 	line-height: 16px; 	background-color: #fff; 	display: block; 	width: 77px; 	height: 17px; 	clear: both; font-size:10px;	margin-top: 5px; 	border: solid 1px #eeeeee; 	font-family: "Microsoft Yahei"; overflow:hidden;}
.header_in .header_rgt dl.down_app .addurl a i {font-size: 14px; 	margin-right: 4px; margin:2px 4px 0 12px;}
.header_in .header_rgt dl.down_app .addurl a:hover {text-decoration: none; 	color: #85c441; 	background-color: #e6f5d1; 	border-color: #85c441}
.header_in .header_rgt dl.down_app .addurl a:hover i {color: #85c441; }

